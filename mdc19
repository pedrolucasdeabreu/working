--Foi encontrado um aumento consider√°vel nas centrais ZRJO09, ZSDR04 no dia 08/02/23. 
--Iremos tentar reprocessar para corrigir o problema mencionado

SELECT DATA,SUM(VOLUME_MOC),SUM(VOLUME_MTA)
FROM RD_T_TRAFEGO_VOZ_MOC_MTA
WHERE DATA BETWEEN '01/02/23' and '10/02/23'
GROUP BY DATA
ORDER BY DATA DESC

SELECT * 
--CENTRAL,VOLUME_MOC ,VOLUME_MTA
FROM RD_T_TRAFEGO_VOZ_MOC_MTA
WHERE DATA = '07/02/23'

UPDATE RD_T_TRAFEGO_VOZ_MOC_MTA
SET VOLUME_MTA = '0'
WHERE DATA = '08/02/23'
AND CENTRAL = 'ZRJO09'

